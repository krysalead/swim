{
  "name": "koteez-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run tsoa-all && tsc --sourceMap false",
    "watch": "tsc -w",
    "load": "artillery run e2e/loadtest.yml -o dist/loadtest.result -q",
    "e2e": "restshooter e2e/dev.cfg",
    "nre": "restshooter e2e/qa.cfg",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "tsoa-all": "npm run swagger && npm run routes",
    "swagger": "tsoa swagger",
    "routes": "tsoa routes",
    "start": "npm run build && node index.js",
    "dev": "nodemon index.js",
    "validation": "ENV=test node index.js",
    "debug": "npm run watch && nodemon --watch dist --debug --debug-brk=5858 index.js",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-hapi": "^1.3.6",
    "bunyan-loggly": "^1.3.5",
    "chalk": "^2.4.1",
    "cls-hooked": "^4.2.2",
    "email-validator": "^2.0.4",
    "good": "7.3.0",
    "good-console": "^7.1.0",
    "good-squeeze": "5.1.0",
    "hapi": "16.6.3",
    "hapi-auth-basic": "^5.0.0",
    "hapi-cls": "^1.1.2",
    "inversify": "4.5.2",
    "inversify-binding-decorators": "3.0.0",
    "jsonwebtoken": "8.2.2",
    "moment": "^2.22.2",
    "mongoose": "4.13.7",
    "password-hash-and-salt": "^0.1.4",
    "reflect-metadata": "0.1.12",
    "tsoa": "2.1.5",
    "typescript-logging": "0.5.0",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/cls-hooked": "^4.2.1",
    "@types/hapi": "16.1.15",
    "@types/mocha": "^5.2.3",
    "@types/mongoose": "4.7.30",
    "@types/password-hash-and-salt": "^0.1.0",
    "@types/uuid": "^3.4.3",
    "artillery": "1.6.0-18",
    "chai": "^4.1.2",
    "http-proxy": "^1.17.0",
    "merge-json": "0.1.0-b.3",
    "mocha": "^5.2.0",
    "mockgoose": "^7.3.5",
    "nodemon": "1.17.5",
    "restshooter": "^1.9.1",
    "ts-node": "^6.1.2",
    "tslint": "5.8.0",
    "typescript": "2.6.2"
  }
}